<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/aqua"
    tools:context=".CollegeListDetails">

    <ImageView
        android:id="@+id/collegeImageView"
        android:layout_width="115dp"
        android:layout_height="0dp"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="41dp" />

    <TextView
        android:id="@+id/collegeNameTextView"
        android:layout_width="222dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/collegeImageView"
        tools:layout_editor_absoluteY="41dp" />

    <RatingBar
        android:id="@+id/collegeRatingBar"
        android:layout_width="237dp"
        android:layout_height="0dp"
        android:numStars="@integer/maxStars"
        android:rating="0"
        android:stepSize=".5"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="77dp" />
</android.support.constraint.ConstraintLayout>